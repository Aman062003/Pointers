#include <iostream>
using namespace std;

int main() {
    int a = 10;
    int* aptr = &a;
    
    cout<<aptr<<endl;
    
    aptr++;
    cout<<aptr<<endl;
    
   // for character------------------------
   
    char ch = 'aman';
    char*cptr = &ch;
    
    cout<<cptr<<endl;
    
    cptr++;
    cout<<cptr<<endl;
    
    
    return 0;
}
